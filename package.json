{
  "name": "amritportfolio",
  "version": "1.0.0",
  "description": "amrit poudels portfolio version 1",
  "main": "portfolio.js",
  "scripts": {
    "watch:sass": "node-sass -w sass/main.scss css/style.css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel watch:sass devserver",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amritpoudel11/myPortfolio.git"
  },
  "author": "Amrit Poudel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amritpoudel11/myPortfolio/issues"
  },
  "homepage": "https://github.com/amritpoudel11/myPortfolio#readme",
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "concat": "^1.0.3",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1"
  }
}
